{
  "name": "hyper-emitter",
  "version": "0.5.0",
  "description": "turn any object to an event emitter using a proxy",
  "unpkg": "./browser.js",
  "main": "./index.js",
  "dependencies": {
    "aka-opts": "0.0.3"
  },
  "devDependencies": {
    "browserify": "^16.2.2",
    "tape": "^4.9.1",
    "tape-run": "^4.0.0"
  },
  "scripts": {
    "bundle": "browserify ./index.js -s hyperEmitter -o ./browser.js",
    "test": "npm run node-test && npm run browser-test",
    "node-test": "tape ./test.js",
    "browser-test": "browserify ./test.browser.js | tape-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiefbiiko/hyper-emitter.git"
  },
  "keywords": [
    "hyper",
    "emitter",
    "object",
    "pojo",
    "watch",
    "observe",
    "proxy",
    "events"
  ],
  "author": "Noah Anabiik Schwarz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiefbiiko/hyper-emitter/issues"
  },
  "homepage": "https://github.com/chiefbiiko/hyper-emitter#readme"
}
